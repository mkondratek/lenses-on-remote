<idea-plugin>

    <id>com.mkondratek.lensesonremote</id>
    <name>Lenses On Remote</name>
    <vendor>mkondratek</vendor>

    <extensions defaultExtensionNs="com.intellij">
        <postStartupActivity
                implementation="com.mkondratek.lensesonremote.PostStartupActivity"/>

        <codeInsight.codeVisionProvider
                id="EditAcceptCodeVisionProvider"
                implementation="com.mkondratek.lensesonremote.providers.EditAcceptCodeVisionProvider"/>
        <codeInsight.codeVisionProvider
                id="EditDiffCodeVisionProvider"
                implementation="com.mkondratek.lensesonremote.providers.EditDiffCodeVisionProvider"/>
        <codeInsight.codeVisionProvider
                id="EditUndoCodeVisionProvider"
                implementation="com.mkondratek.lensesonremote.providers.EditUndoCodeVisionProvider"/>
        <codeInsight.codeVisionProvider
                id="EditRetryCodeVisionProvider"
                implementation="com.mkondratek.lensesonremote.providers.EditRetryCodeVisionProvider"/>
    </extensions>

</idea-plugin>
